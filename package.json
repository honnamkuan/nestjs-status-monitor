{
  "name": "nestjs-status-monitor",
  "version": "0.2.1",
  "description": "Realtime Monitoring for Express-based NestJS applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honnamkuan/nestjs-status-monitor.git"
  },
  "author": "honnamkuan",
  "license": "Apache-2.0",
  "keywords": [
    "nestjs",
    "status",
    "monitoring",
    "node"
  ],
  "devDependencies": {
    "@nestjs/common": "^7.6.7",
    "@nestjs/core": "^7.6.7",
    "@nestjs/testing": "^7.6.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "nestjs-config": "^1.4.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@nestjs/websockets": "^7.6.7",
    "axios": "^0.21.1",
    "handlebars": "4.7.6",
    "on-headers": "^1.0.2",
    "pidusage": "^2.0.21",
    "reflect-metadata": "^0.1.13",
    "request-promise-native": "^1.0.8",
    "socket.io": "^2.4.1"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "start:dev": "tsc --watch --declaration",
    "build": "rm -rf ./dist && tsc --declaration",
    "format": "prettier src/**/*.ts --ignore-path ./.prettierignore --write && git status",
    "prepublish": "npm run format && npm run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/honnamkuan/nestjs-status-monitor/issues"
  },
  "homepage": "https://github.com/honnamkuan/nestjs-status-monitor#readme",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "optionalDependencies": {
    "event-loop-stats": "^1.3.0"
  }
}
